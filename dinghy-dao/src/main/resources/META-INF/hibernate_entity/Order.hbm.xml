<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
    <class name="com.dinghy.domain.order.Order" table="order1">
        <id name="id" type="java.lang.Long" column="ORDER_ID">
            <!-- 用来指明主键的生成方式 -->
            <generator class="sequence" >
                <param name="sequence">order1_seq</param>
            </generator>
        </id>
        <property name="merNo">
            <column name="MerNo">
                <comment>商户号</comment>
            </column>
        </property>
        <property name="billNo">
            <column name="BillNo">
                <comment>订单号</comment>
            </column>
        </property>
        <property name="yemadaiOrderNo">
            <column name="YemadaiOrderNo">
                <comment>订单号</comment>
            </column>
        </property>
        <property name="amount">
            <column name="Amount">
                <comment>金额</comment>
            </column>
        </property>
        <property name="succeed">
            <column name="Succeed">
                <comment>状态码</comment>
            </column>
        </property>
        <property name="result">
            <column name="Result">
                <comment>返回结果</comment>
            </column>
        </property>
        <property name="signInfo">
            <column name="SignInfo">
                <comment>签名</comment>
            </column>
        </property>
        <property name="ip">
            <column name="Ip">
                <comment>回调IP</comment>
            </column>
        </property>
        <property name="createTime">
            <column name="CreateTime">
                <comment>订单创建时间</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>