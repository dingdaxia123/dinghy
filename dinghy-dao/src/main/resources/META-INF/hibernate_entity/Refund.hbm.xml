<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field">
    <class name="com.dinghy.domain.order.Refund" table="REFUND">
        <id name="id" type="java.lang.Long" column="REFUND_ID">
            <!-- 用来指明主键的生成方式 -->
            <generator class="sequence" >
                <param name="sequence">refund_seq</param>
            </generator>
        </id>
        <property name="merCode">
            <column name="MerCode">
                <comment>商户号</comment>
            </column>
        </property>
        <property name="createTime">
            <column name="CreateTime">
                <comment>订单创建时间</comment>
            </column>
        </property>
        <property name="orderNumber">
            <column name="OrderNumber">
                <comment>订单号</comment>
            </column>
        </property>
        <property name="orderTime">
            <column name="OrderTime">
                <comment>退款时间</comment>
            </column>
        </property>
        <property name="orderAmount">
            <column name="OrderAmount">
                <comment>原金额</comment>
            </column>
        </property>
        <property name="refundAmount">
            <column name="RefundAmount">
                <comment>退款金额</comment>
            </column>
        </property>
        <property name="sign">
            <column name="Sign">
                <comment>签名</comment>
            </column>
        </property>
        <property name="remark">
            <column name="Remark">
                <comment>备注</comment>
            </column>
        </property>
        <property name="refundStatus">
            <column name="RefundStatus">
                <comment>退款状态</comment>
            </column>
        </property>
        <property name="refundNumber">
            <column name="RefundNumber">
                <comment>退款订单号</comment>
            </column>
        </property>
        <property name="ip">
            <column name="IP">
                <comment>回调IP</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>